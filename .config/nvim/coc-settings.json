{
	"coc.preferences.formatOnSaveFiletypes": [
		"css",
		"html",
		"typescript",
		"javascript",
		"php",
		"json",
		"ocaml",
		"clojure",
		"rust"
	],
	"languageserver": {
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		},
		"ocaml": {
			"command": "ocaml-language-server",
			"args": ["--stdio"],
			"filetypes": ["ocaml", "reason"]
		}
	}
}
