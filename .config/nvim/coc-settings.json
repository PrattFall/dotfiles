{
  "coc.preferences.formatOnSaveFiletypes": [
    "clojure",
    "css",
    "html",
    "javascript",
    "json",
    "ocaml",
    "php",
    "python",
    "rust",
    "scss",
    "sql",
    "typescript"
  ],
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "ocaml": {
      "command": "ocaml-language-server",
      "args": ["--stdio"],
      "filetypes": ["ocaml", "reason"]
    }
  }
}
